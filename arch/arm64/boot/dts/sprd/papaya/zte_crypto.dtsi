/* add zte crypto start */
&spi0 {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;
	spi_ssx1207@0 {
		compatible = "veb,veba5-spi";
		reg = <0>;
		spi-max-frequency = <1000000>;
		veb_spi_vebv3,reset-gpio = <&ap_gpio 131 0x0>;
		veb_spi_vebv3,status-gpio = <&ap_gpio 110 0x0>;
		veb_spi_vebv3,wake-gpio = <&ap_gpio 109 0x0>;
	};
};


/* add zte crypto end */
